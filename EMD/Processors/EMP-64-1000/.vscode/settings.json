{
    "code-runner.executorMapByFileExtension": {
        ".vb": "cd $dir && vbc /nologo $fileName && $dir$fileNameWithoutExt",
        ".vbs": "cscript //Nologo",
        ".scala": "scala",
        ".jl": "julia",
        ".cr": "crystal",
        ".ml": "ocaml",
        ".exs": "elixir",
        ".hx": "haxe --cwd $dirWithoutTrailingSlash --run $fileNameWithoutExt",
        ".rkt": "racket",
        ".scm": "csi -script",
        ".ahk": "autohotkey",
        ".au3": "autoit3",
        ".kt": "cd $dir && kotlinc $fileName -include-runtime -d $fileNameWithoutExt.jar && java -jar $fileNameWithoutExt.jar",
        ".kts": "kotlinc -script",
        ".dart": "dart",
        ".pas": "cd $dir && fpc $fileName && $dir$fileNameWithoutExt",
        ".pp": "cd $dir && fpc $fileName && $dir$fileNameWithoutExt",
        ".d": "cd $dir && dmd $fileName && $dir$fileNameWithoutExt",
        ".hs": "runhaskell",
        ".nim": "nim compile --verbosity:0 --hints:off --run",
        ".csproj": "dotnet run --project",
        ".fsproj": "dotnet run --project",
        ".lisp": "sbcl --script",
        ".kit": "kitc --run",
        ".v": "v run",
        ".vsh": "v run",
        ".sass": "sass --style expanded",
        ".cu": "cd $dir && nvcc $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
        ".asm": "python tools/Assembler.py $fileName -o $fileNameWithoutExt; python Program.py $fileNameWithoutExt; python tools/Remove.py $fileNameWithoutExt"
    },
    "code-runner.executorMapByGlob": {
        "pom.xml": "cd $dir && mvn clean package"
    },
    "editor.tokenColorCustomizations": {
        "textMateRules": [{
            "scope": "keyword.toplevel.asm",
            "settings": {
                "foreground": "#E5CD52"
            }
        }, {
            "scope": "control.section.asm",
            "settings": {
                "foreground": "#EF7C2A"
            }
        }, {
            "scope": "bytes.asm",
            "settings": {
                "foreground": "#EB3D54"
            }
        }, {
            "scope": "bytes.asm",
            "settings": {
                "foreground": "#EB3D54"
            }
        }, {
            "scope": "datatype.hex.asm",
            "settings": {
                "foreground": "#EF7C2A"
            }
        }, {
            "scope": "datatype.binary.asm",
            "settings": {
                "foreground": "#EF7C2A"
            }
        }, {
            "scope": "datatype.hexpre.asm",
            "settings": {
                "foreground": "#EB3D54"
            }
        }, {
            "scope": "datatype.binpre.asm",
            "settings": {
                "foreground": "#EB3D54"
            }
        }, {
            "scope": "datatype.unknown.asm",
            "settings": {
                "foreground": "#E5CD52"
            }
        }, {
            "scope": "datatype.reserve.asm",
            "settings": {
                "foreground": "#EB3D54"
            }
        }, {
            "scope": "datatype.integer.asm",
            "settings": {
                "foreground": "#EF7C2A"
            }
        }, {
            "scope": "datatype.prefix.asm",
            "settings": {
                "foreground": "#E5CD52"
            }
        }, {
            "scope": "datatype.variable.asm",
            "settings": {
                "foreground": "#CBCDD2"
            }
        }, {
            "scope": "control.label.asm",
            "settings": {
                "foreground": "#e5cc5279"
            }
        }, {
            "scope": "control.procedure.asm",
            "settings": {
                "foreground": "#EB3D54"
            }
        }, {
            "scope": "control.start.asm",
            "settings": {
                "foreground": "#EF7C2A"
            }
        }, {
            "scope": "control.procedure.name.asm",
            "settings": {
                "foreground": "#E5CD52"
            }
        }, {
            "scope": "sugar.comment.todo",
            "settings": {
                "foreground": "#78BD65"
            }
        }]
    }
}
